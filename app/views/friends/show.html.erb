<p id="notice"><%= notice %></p>

<div class="jumbotron">
  <h1 class="display-4">
    <%= @friend.first_name %>
    <%= @friend.last_name %>
  </h1>
  <p class="lead">
<strong>Email:</strong><%= @friend.email %> |
<strong>Phone:</strong><%= @friend.phone %> |
<strong>Twitter:</strong><%= @friend.twitter %>
    
  </p>
  <p class="lead">
    <%= link_to 'Edit', edit_friend_path(@friend), class: 'btn btn-secondary' %> 
    <%= link_to 'Back', friends_path, class: 'btn btn-outline-primary' %>
  </p>
</div>